    "graphql-codegen-typescript-common": "^0.15.2"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "copyfiles": "^2.1.0",
    "graphql": "14.0.2",
    "graphql-codegen-plugin-handlebars-helpers": "0.15.2",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": false
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
