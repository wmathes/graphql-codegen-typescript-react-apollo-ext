{
  "name": "graphql-codegen-typescript-react-apollo-named",
  "description": "",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "postbuild": "cd src && copyfiles \"*.handlebars\" ../dist",
